#include<stdio.h>      // Link section
void main()
     {
         // Declare variables
       int num1,num2,i,hcf=1,min;
         // Get numbers from user
       printf("Enter the numbers\n");
       scanf("%d %d",&num1,&num2);
         // Find minimum of the numbers
       min=(num1<num2)?num1:num2;
       for(i=1;i<=min;i++)
            {
                if(num1%i==0 && num2%i==0)
                    {
                       hcf=i;
                    }
            }
      printf("\nHCF is %d",hcf);
    }
