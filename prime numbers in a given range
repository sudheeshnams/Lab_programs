#include<stdio.h>    // Link section
void main()
     {
         // declare variables
       int n,i,lower,upper,prime;
       printf("Enter lower limit");
       scanf("%d",&lower);
       printf("\nEnter upper limit");
       scanf("%d",upper);
       printf("\nPrime numbers list is");
       for(n=lower+1;n<upper;n++)
            {
               prime=1;
               for(i=2;i<n/2;i++)
                    {
                        if(n%1==0)
                          {
                             prime=0;
                             break;
                          }
                        if(prime)
                        printf("\t%d",n);
                    }
            }
   }
