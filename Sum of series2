#include<stdio.h>   // Link section 
void main()
    {
         // Declare variables 
     int n,sum=0;
         // Get value from the user 
     printf("Enter the limit");
     scanf("%d",&n);
     for(int i=1;i<=n;i++)
        {
         sum+=1/i;
        }
        printf("\n the sum of series=%d",sum);
    }       
