#include<stdio.h>   // Link section
void main()
      {
          // Declare variables 
        int x,y;
          /* Get values from user */
        printf("Enter values for x,y\n);
        scanf("%d %d",&x,&y);
        if(x>0 && y>0)
          printf("The point lies in first quadrant ");
        else if(x<0 && y>0)
          printf("The point lies in second quadrant ");
        else if(x<0 && y<0)
          printf("The point lies in third quadrant ");
        else if(x>0 && y<0)
          printf("The point lies in fourth quadrant ");
        else
          printf("The point lies at the origin ");
     }
