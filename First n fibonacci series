#include<studio.h>  // Link section 
void main()

      {
        /* Declare variables and initialize t1 and t2 for first series */
         int num,t1=0,t2=1,t3,count=0;

        /* Get value of num  from user */
         printf("\nEnter value of num");
         scanf("%d",&num);

         printf("First %d fibonacci numbers are...\n",num);
         printf("%d\n",t1);
         printf("%d\n",t2);
         count=2;   // t1 and t2 are already used
         while(count<num)
            {
               t3=t1+t2;
               count++;
               printf("%d\n",t3);
               t1=t2;
               t2=t3;
            }

     
      }
