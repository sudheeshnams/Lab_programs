#include<stdio.h>   // Link section 
void main()
     {
         // Declare variables 
       int num,i;
         // Get value from user 
       printf("Enter number");
       scanf("%d",&num);
       printf("Multiplication table of %d is \n",num);
       printf("........\n");
       for(i=1;i<=10;i++)
           {
               printf("%d * %d = %d \ n",num,i,num*i);
           }
     }
