#include<studio.h>  // Link section 
void main()

      {
        /* Declare variables and initialize t1 and t2 for first series */
         int i,n,t1=0,t2=1,nextterm;

        /* Get the number of terms from user */
         printf("\nEnter number of terms");
         scanf("%d",&n);

         printf("Fibonacci series:");
         for(i=1;i<=n;++i)
            {
               printf("%d",t1);
               nextterm=t1+t2;
               t1=t2;
               t2=nextterm;
            }

     
      }
