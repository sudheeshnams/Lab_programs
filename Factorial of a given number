#include<studio.h>  // Link section 
void main()

      {
        //Declare variables
         int c,n,f=1;

        /* Get value from user */
         printf("Enter number\n");
         scanf("%d",&n);

         for(c=1;c<=n;c++)
             {
                 f=f*c;
                 printf("Factorial of %d=%d\n",n,f);
             }

     
      }
