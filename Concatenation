#include<stdio.h>
void main()
     {
       char s1[50],s2[50],s3[100];
       int i=0,j=0;
       printf("Enter string ending with $");
       scanf("%s",s1);
       printf("\nEnter another string ending with $");
       scanf("%s",s2);
       while(s1[i]!='\0')
           {
              s3[j]=s1[i];
              i++;
              j++;
           }
      i=0;
      while(s2[i]!='\0')
           {
              s3[j]=s2[i];
              i++;
              j++;
           }
     s3[j]='\0';
     printf("\nConcatenated string is %s",s3);
   }
