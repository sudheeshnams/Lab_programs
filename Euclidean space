#include<stdio.h>
struct distance{
                 int x;
                 int y;
               }a,b,c;
             
void main()
    {
      printf("Enter x and y of first point");
      scanf("%d %d",&a.x,&a.y);
      printf("\nEnter x and y for second point");
      scanf("%d %d",&b.x,&b.y);
      c.x=a.x+b.x;
      c.y=a.y+b.y;
      printf("\nSum of of two distance values is %d and %d",c.x,c.y);
   }
