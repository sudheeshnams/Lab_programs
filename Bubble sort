#include<stdio.h>
void main()
    {
       int n,a[20],pass,i,temp;
       printf("\nEnter number of elements");
       scanf("%d",&n);
       printf("\nEnter array elements");
       for(i=0;i<n;i++)
          {
            scanf("%d",&a[i]);
          }
      printf("\nElements are");
      for(i=0;i<n;i++)
          {
            printf("%d\t",a[i]);
          }
      for(pass=0;pass<n-1;pass++)
          {
            for(i=0;i<n-1;i++)
              {
                 if(a[i]>a[i+1])
                   {
                      temp=a[i];
                      a[i]=a[i+1];
                      a[i+1]=temp;
                   }
              }
          }
     printf("\nSorted array is");
     for(i=0;i<n;i++)
          {
            printf("%d",a[i]);
          }
 }
      
            
             
