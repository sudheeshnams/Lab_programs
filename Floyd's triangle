#include<stdio.h>    // Link section 
void main()
      {
          // Declare variables 
        int rows,i,j,num=1;
        printf("Enter number of rows");
        scanf("%d",&rows);
        printf("\n Printing Floyd's triangle\n");
        for(i=1;i<=rows;i++)
            {
                for(j=1;j<=1;++j)
                    {
                       printf("%d",num);
                       ++num;
                    }
                printf("\n");
            }
      }
