#include<studio.h>  // Link section 
void main()

      {
        //Declare variables
         int i,sum=0;

         printf("First 10 natural numbers is \n");
         for(i=1;i<=10;i++)
             {
                 sum=sum+i;
                 printf("%d",i);
             }

     
         printf("\nThe sum is %d \n",sum);
      }
